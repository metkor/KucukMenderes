<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Küçük Menderes</name>
    <description>
        Küçük Menderes Gazetesi Mobil Uygulaması
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Küçük Menderes
    </author>
    <content src="index.html" />
    <access origin="*" />
    <engine name="android" spec="~7.1.2" />
	<splash src="splash.png" platform="android" />
<splash src="splash.png" platform="android" qualifier="ldpi" />
<splash src="splash.png" platform="android" qualifier="mdpi" />
<splash src="splash.png" platform="android" qualifier="hdpi" />
<splash src="splash.png" platform="android" qualifier="xhdpi" />
<splash src="splash.png" platform="android" qualifier="fr-xhdpi" />
<splash src="splash.png" platform="android" qualifier="port-xxhdpi" />
<splash src="splash.png" platform="android" qualifier="land-xxhdpi" />
<splash src="splash.png" platform="android" qualifier="xxxhdpi" />	    

<icon src="icon.png" />
<preference name="SplashScreen" value="screen" />
 <preference name="SplashScreenDelay" value="10000" />
<plugin name="cordova-plugin-inappbrowser" spec="~3.0.0" />
<plugin name="cordova-plugin-splashscreen" />
<access origin="http://www.kucukmenderes.com.tr" />


</widget>
